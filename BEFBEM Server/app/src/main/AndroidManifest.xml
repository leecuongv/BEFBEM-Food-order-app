<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.foa.orderfoodserver">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.foa.orderfoodserver.Demo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.foa.orderfoodserver.MainActivity">

        </activity>
        <activity android:name="com.foa.orderfoodserver.SignIn" />
        <activity
            android:name="com.foa.orderfoodserver.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
        <activity android:name="com.foa.orderfoodserver.FoodList" />
        <activity android:name="com.foa.orderfoodserver.OrderStatus" />

        <service
            android:name="com.foa.orderfoodserver.Service.ListenOrder"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>